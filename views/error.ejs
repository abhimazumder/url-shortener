      <div class="jumbotron jumbotron-fluid border rounded">
        <div class="container">
          <h1 class="display-4"><%= error.status %> Error</h1>
          <p class="lead"><%= error.message %></p>
          <p>Go back to <a href="http://localhost:3001/" style="color:whitesmoke">home.</a></p>
        </div>
      </div>
    </div>
